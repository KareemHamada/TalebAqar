

<!doctype html>
<html class="no-js" lang="ar">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@SiteSettings.Value.WebsiteName</title>
    <meta name="robots" content="noindex, follow" />
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <meta property="og:url" content="https://talebaqar.com" />
    <meta property="og:title" content="Taleb Aqar - طالب عقار" />
    <meta property="og:description" content="Looking for a dream home Without a real estate broker" />
    <meta property="og:image" content="@SiteSettings.Value.Logo" />

    <meta property="al:web:url" content="https://talebaqar.com/" />




    <!-- Place favicon.png in the root directory -->
    <link rel="shortcut icon" href="~/Uploads/DefaultImages/favicon.png" type="image/x-icon" />

    <!-- Font Icons css -->
    <link rel="stylesheet" href="~/Frontend/css/font-icons.css">
    <!-- plugins css -->
    <link rel="stylesheet" href="~/Frontend/css/plugins.css">
    <!-- Main Stylesheet -->
    <link rel="stylesheet" href="~/Frontend/css/style.css">
    <!-- Responsive css -->
    <link rel="stylesheet" href="~/Frontend/css/responsive.css">


    @RenderSection("Styles", required: false)
</head>

<body>
   

    <!-- Add your site or application content here -->

<!-- Body main wrapper start -->
<div class="body-wrapper">
        <!-- HEADER AREA START (header-5) -->
        <header class="ltn__header-area ltn__header-5 ltn__header-transparent--- gradient-color-4---">
            <!-- ltn__header-middle-area start -->
            <div class="ltn__header-middle-area ltn__header-sticky ltn__sticky-bg-white">
                <div class="container">
                    <div class="row">
                        <div class="col">
                            <div class="site-logo-wrap">
                                <div class="site-logo">
                                    <a href="/"><img src="@SiteSettings.Value.Logo" alt="Logo"></a>
                                </div>
                                
                            </div>
                        </div>
                        <div class="col header-menu-column">
                            <div class="header-menu d-none d-xl-block">
                                <nav>
                                    <div class="ltn__main-menu">
                                        <ul>
                                            <li><a href="/">الرئيسية</a></li>
                                            <li><a href="/Home/PropertiesGridForSale">جميع العقارات المتاحة للبيع</a></li>
                                            <li><a href="/Home/PropertiesGridForRent">جميع العقارات المتاحة للإيجار</a></li>
                                        </ul>
                                    </div>
                                </nav>
                            </div>
                        </div>
                        <div class="col ltn__header-options ltn__header-options-2 mb-sm-20">
                            <!-- mini-cart -->
                            <div class="mini-cart-icon">
                                <a href="/home/Wishlist">
                                    <i class="fas fa-heart"></i>
                                    <sup>0</sup>
                                </a>
                            </div>
                            <!-- mini-cart -->
                            <!-- Mobile Menu Button -->
                            <div class="mobile-menu-toggle d-xl-none">
                                <a href="#ltn__utilize-mobile-menu" class="ltn__utilize-toggle">
                                    <svg viewBox="0 0 800 600">
                                        <path d="M300,220 C300,220 520,220 540,220 C740,220 640,540 520,420 C440,340 300,200 300,200" id="top"></path>
                                        <path d="M300,320 L540,320" id="middle"></path>
                                        <path d="M300,210 C300,210 520,210 540,210 C740,210 640,530 520,410 C440,330 300,190 300,190" id="bottom" transform="translate(480, 320) scale(1, -1) translate(-480, -318) "></path>
                                    </svg>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- ltn__header-middle-area end -->
        </header>
        <!-- HEADER AREA END -->

        <!-- Utilize Mobile Menu Start -->
        <div id="ltn__utilize-mobile-menu" class="ltn__utilize ltn__utilize-mobile-menu">
            <div class="ltn__utilize-menu-inner ltn__scrollbar">
                <div class="ltn__utilize-menu-head">
                    <div class="site-logo">
                        <a href="/"><img src="@SiteSettings.Value.Logo" alt="Logo"></a>
                    </div>
                    <button class="ltn__utilize-close">×</button>
                </div>
                
                <div class="ltn__utilize-menu">
                    <ul>
                        <li><a href="/">الرئيسية</a></li>
                        <li><a href="/Home/PropertiesGridForSale">جميع العقارات المتاحة للبيع</a></li>
                        <li><a href="/Home/PropertiesGridForRent">جميع العقارات المتاحة للإيجار</a></li>
                    </ul>
                </div>
                <div class="ltn__utilize-buttons ltn__utilize-buttons-2">
                    <ul>
                        <li>
                            <a href="/home/wishlist" title="Wishlist">
                                <span class="utilize-btn-icon mini-cart-icon">
                                    <i class="far fa-heart"></i>
                                    <sup>0</sup>
                                </span>
                               مفضلتى
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="ltn__social-media-2">
                    <ul>
                        @if (!string.IsNullOrEmpty(SiteSettings.Value.FacebookLink))
                        {
                            <li><a href="@SiteSettings.Value.FacebookLink" title="Facebook"><i class="fab fa-facebook-f"></i></a></li>
                        }
                        @if (!string.IsNullOrEmpty(SiteSettings.Value.TwitterLink))
                        {
                            <li><a href="@SiteSettings.Value.TwitterLink" title="Twitter"><i class="fab fa-twitter"></i></a></li>
                        }
                        @if (!string.IsNullOrEmpty(SiteSettings.Value.InstgramLink))
                        {
                            <li><a href="@SiteSettings.Value.InstgramLink" title="Instgram"><i class="fab fa-instagram"></i></a></li>
                        }
                        @if (!string.IsNullOrEmpty(SiteSettings.Value.LinkedinLink))
                        {
                            <li><a href="@SiteSettings.Value.LinkedinLink" title="Linkedin"><i class="fab fa-linkedin"></i></a></li>
                        }
                        @if (!string.IsNullOrEmpty(SiteSettings.Value.YoutubeLink))
                        {
                            <li><a href="@SiteSettings.Value.YoutubeLink" title="Youtube"><i class="fab fa-youtube"></i></a></li>
                        }
                    </ul>
                </div>
            </div>
        </div>
        <!-- Utilize Mobile Menu End -->

        <div class="ltn__utilize-overlay"></div>

   
    @RenderBody();

        <!-- CALL TO ACTION START (call-to-action-6) -->
        <div class="ltn__call-to-action-area call-to-action-6 before-bg-bottom" data-bs-bg="img/1.jpg--">


            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="call-to-action-inner call-to-action-inner-6 ltn__secondary-bg position-relative text-center---">
                            <div class="coll-to-info text-color-white">
                                @if (!string.IsNullOrEmpty(SiteSettings.Value.WebsiteDescription))
                                {
                                    <h2>@SiteSettings.Value.WebsiteDescription</h2>
                                }else{
                                    <h2>مع طالب عقار حلك بسيط بدون وسيط</h2>
                                }

                                
                            </div>
                            <div class="btn-wrapper">
                                <a class="btn btn-effect-3 btn-white" href="/Home/PropertiesGridForSale">اكتشف العقارات<i class="icon-left-arrow"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- CALL TO ACTION END -->



    <!-- FOOTER AREA START -->
    <footer class="ltn__footer-area">

            <div b-38w4zr4b1g class="footer-top-area  section-bg-2 plr--5">
            </div>

        <div class="ltn__copyright-area ltn__copyright-2 section-bg-7  plr--5">
            <div class="container-fluid ltn__border-top-2">
                <div class="row">
                    <div class="col-md-6 col-12">
                        <div class="ltn__copyright-design clearfix">
                            <p>جميع الحقوق محفوظة @@ @SiteSettings.Value.WebsiteName <span>@DateTime.Now.Year</span></p>
                        </div>
                    </div>
                    <div class="col-md-6 col-12 align-self-center">
                        <div class="ltn__copyright-menu text-end">
                            <ul>
                                    @if (!string.IsNullOrEmpty(SiteSettings.Value.FacebookLink))
                                    {
                                        <li><a href="@SiteSettings.Value.FacebookLink" title="Facebook"><i class="fab fa-facebook-f"></i></a></li>
                                    }
                                    @if (!string.IsNullOrEmpty(SiteSettings.Value.TwitterLink))
                                    {
                                        <li><a href="@SiteSettings.Value.TwitterLink" title="Twitter"><i class="fab fa-twitter"></i></a></li>
                                    }
                                    @if (!string.IsNullOrEmpty(SiteSettings.Value.InstgramLink))
                                    {
                                        <li><a href="@SiteSettings.Value.InstgramLink" title="Instgram"><i class="fab fa-instagram"></i></a></li>
                                    }
                                    @if (!string.IsNullOrEmpty(SiteSettings.Value.LinkedinLink))
                                    {
                                        <li><a href="@SiteSettings.Value.LinkedinLink" title="Linkedin"><i class="fab fa-linkedin"></i></a></li>
                                    }
                                    @if (!string.IsNullOrEmpty(SiteSettings.Value.YoutubeLink))
                                    {
                                        <li><a href="@SiteSettings.Value.YoutubeLink" title="Youtube"><i class="fab fa-youtube"></i></a></li>
                                    }
                                    <li>@SiteSettings.Value.Email</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- FOOTER AREA END -->

    <!-- MODAL AREA START (Quick View Modal) -->
    <div class="ltn__modal-area ltn__quick-view-modal-area">
        <div class="modal fade" id="quick_view_modal" tabindex="-1">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                            <!-- <i class="fas fa-times"></i> -->
                        </button>
                    </div>
                    <div class="modal-body">
                         <div class="ltn__quick-view-modal-inner">
                             <div class="modal-product-item">
                                <div class="row">
                                    <div class="col-lg-6 col-12">
                                        <div class="modal-product-img">
                                                <img id="modal-product-image" src="" alt="#">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-12">
                                        <div class="modal-product-info">
                                            <div class="d-flex align-items-center justify-content-space-around">
                                                <h3 id="modal-product-type" class="bg-red mb-0 p-1"></h3>
                                                
                                                <span style="display: inline-block; width: 4ch;"></span>

                                                <span id="modal-product-status" class="bg-orange p-1"></span>
                                            </div>
                                                <span id="modal-product-id" class="d-none"></span>

                                            <div class="product-price">
                                                    <span id="modal-product-price"></span>
                                            </div>
                                            <hr>
                                            <div class="modal-product-brief">
                                                    <p id="modal-product-description"></p>
                                            </div>
                                         
                                            <!-- <hr> -->
                                            <div class="ltn__product-details-menu-3">
                                                <ul>
                                                    <li>
                                                            <a href="#" class="" title="Wishlist" data-bs-toggle="modal" data-bs-target="#liton_wishlist_modal"
                                                               onclick="addItemToWishlist()">
                                                                
                                                            <i class="far fa-heart"></i>
                                                            <span>إضافة إلى المفضلة</span>
                                                        </a>
                                                    </li>
                                                </ul>
                                            </div>
                                           
                                            <hr>
                                                <label class="float-end mb-0"><a id="modal-product-link" class="text-decoration" href=""><small>مشاهدة التفاصيل</small></a></label>
                                        </div>
                                    </div>
                                </div>
                             </div>
                         </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- MODAL AREA END -->

   

    <!-- MODAL AREA START (Wishlist Modal) -->
    <div class="ltn__modal-area ltn__add-to-cart-modal-area">
        <div class="modal fade" id="liton_wishlist_modal" tabindex="-1">
            <div class="modal-dialog modal-md" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                         <div class="ltn__quick-view-modal-inner">
                             <div class="modal-product-item">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="modal-product-img">
                                                <img src="~/Uploads/DefaultImages/default-image.png" alt="image" id="wishlistModalImage">
                                        </div>
                                         <div class="modal-product-info">
                                         <p class="added-cart" id="messageAdding"></p>
                                            <div class="btn-wrapper">
                                                <a href="/home/wishlist" class="theme-btn-1 btn btn-effect-1">قائمة مفضلاتى</a>
                                            </div>
                                         </div>
                                    </div>
                                </div>
                             </div>
                         </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- MODAL AREA END -->

</div>
<!-- Body main wrapper end -->

    <!-- preloader area start -->
    <div class="preloader d-none" id="preloader">
        <div class="preloader-inner">
            <div class="spinner">
                <div class="dot1"></div>
                <div class="dot2"></div>
            </div>
        </div>
    </div>
    <!-- preloader area end -->


    <!-- All JS Plugins -->
    <script src="~/Frontend/js/plugins.js"></script>
    <!-- Main JS -->
    <script src="~/Frontend/js/main.js"></script>

    <script src="~/custom/js/custom.js"></script>

    
    @RenderSection("Scripts", required: false)

</body>
</html>

